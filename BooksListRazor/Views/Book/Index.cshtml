@model IEnumerable<BooksListRazor.Models.Book>
@{
    ViewData["Title"] = "Index";
}
<div class="container">
    <div class="row">
        <div class="col-8 offset-2">
            <h1 class="text-center text-danger">Books List</h1>
            <table class="table table-bordered">
                <tr class="text-center">
                    <th>Id</th>
                    <th>Name</th>
                    <th>Author</th>
                    <th>ISBN</th>
                </tr>
                @foreach(var d in Model)
            {
                <tr class="text-center">
                    <td>@d.Id</td>
                    <td>@d.Name</td>
                    <td>@d.Author</td>
                    <td>@d.ISBN</td>
                </tr>
            }
            </table>
        </div>
    </div>
</div>



